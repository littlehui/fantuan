<?page title="饭团GO 1.0" contentType="text/html;charset=UTF-8"?>
<?init class="org.zkoss.zkplus.databind.AnnotateDataBinderInit" root="./userPriceLogWin"?>
<zk>
    <window title="" id="userPriceLogWin" apply="com.littlehui.fantuan.controller.UserPriceLogController">
        <tabbox>
            <tabs>
                <tab label="消费信息" id="consumeTab" width="33%"></tab>
                <tab label="充值信息" id="rechargeTab" width="33%"></tab>
                <tab label="转账信息" id="exchangeTab" width="34%"></tab>
            </tabs>
            <tabpanels>
                <tabpanel>
                    <include src="/zul/mvc/consume/consumeList.zul"></include>
                </tabpanel>
                <tabpanel>
                    <include src="/zul/mvc/recharge/rechargeList.zul"></include>
                </tabpanel>
                <tabpanel>
                    <include src="/zul/mvc/exchange/exchangeList.zul"></include>
                </tabpanel>
            </tabpanels>

        </tabbox>
    </window>
</zk>